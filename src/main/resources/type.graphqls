type Match {
    score: Int
    matchId: String!
    leagueName: String!
    minutesOfMatch: String!
    home: Team!
    away: Team!
    betName: String!
}

type Team {
    name: String!
    score: Int!
    redCard: Int
    cornerKick: Int
    shotOnGoal: Int
    shotOffGoal: Int
    ballPossession: String
    logo: String
}